{
  "name": "chat-room",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "dev": "vite --host --open",
    "format": "prettier --write src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
  },
  "lint-staged": {
    "*.{vue,ts}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@julr/unocss-preset-forms": "^0.1.0",
    "@types/js-cookie": "^3.0.6",
    "@unocss/reset": "^0.58.0",
    "@vee-validate/i18n": "^4.12.3",
    "@vee-validate/yup": "^4.12.3",
    "@vue-hooks-plus/use-request-plugins": "^1.8.0",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vee-validate": "^4.12.3",
    "vue": "^3.3.12",
    "vue-draggable-plus": "^0.3.2",
    "vue-hooks-plus": "^1.8.6",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5",
    "vue-toastification": "2.0.0-rc.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@faker-js/faker": "^8.3.1",
    "@iconify/json": "^2.2.158",
    "@intlify/unplugin-vue-i18n": "^1.6.0",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.1",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue-hooks-plus/resolvers": "^1.2.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.8.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.1",
    "typescript": "~5.2.0",
    "unocss": "^0.58.0",
    "unocss-preset-extra": "^0.5.3",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-icons": "^0.18.1",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-inspector": "^1.0.2",
    "vite": "^4.5.1",
    "vite-bundle-visualizer": "^0.11.0",
    "vite-plugin-i18n-resources": "^1.0.3",
    "vite-plugin-inspect": "^0.8.1",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-mock-dev-server": "^1.4.3",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-plugin-vue-devtools": "1.0.0-rc.7",
    "vite-plugin-vue-inspector": "^4.0.2",
    "vite-tsconfig-paths": "^4.2.2",
    "vue-tsc": "^1.8.25"
  }
}